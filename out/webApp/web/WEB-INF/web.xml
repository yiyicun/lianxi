<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <servlet-name>jndAdd</servlet-name>
        <servlet-class>xingyun.action.JndebAddAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>jndAdd</servlet-name>
        <url-pattern>/jndAdd</url-pattern>
    </servlet-mapping>

    <!-- 加拿大28统计 -->
    <servlet>
        <servlet-name>jndEBstats</servlet-name>
        <servlet-class>xingyun.action.JndEBStatsAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>jndEBstats</servlet-name>
        <url-pattern>/jndEBstats</url-pattern>
    </servlet-mapping>

    <!-- 加拿大趋势 -->
    <servlet>
        <servlet-name>jndtrend</servlet-name>
        <servlet-class>xingyun.action.JNDEBtrendAction</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>jndtrend</servlet-name>
        <url-pattern>/jndtrend</url-pattern>
    </servlet-mapping>



    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>


</web-app>